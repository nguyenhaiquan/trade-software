var v_version = 0;
var v_browser = "";

function detectBrowserVersion() {
    if ($.browser.msie) {
        v_version = $.browser.version;
        v_browser = "IE";
    }
    else if ($.browser.chrome) {
        v_version = $.browser.version;
        v_browser = "CR";
    }
    else if ($.browser.mozilla) {
        v_version = $.browser.version;
        v_browser = "FF";
    }
    else if ($.browser.safari) {
        v_version = $.browser.version;
        v_browser = "SF";
    }
    else if ($.browser.opera) {
        v_version = $.browser.version;
        v_browser = "OP";
    }
}

